  "require": {
    "php": ">=5.6",
    "tcdent/php-restclient": "^0.1.7",
    "netresearch/jsonmapper": "^1.3"
  },
  
   "repositories":[
        {
            "type":"git",
            "url":"https://github.com/DanilaD/npilook"
        }
    ],
 
  "npilook": "dev-master",
  "minimum-stability": "dev",
  
  "autoload": {
    "psr-0": {
      "npilookup\\": "src/"
    }
  }
}
